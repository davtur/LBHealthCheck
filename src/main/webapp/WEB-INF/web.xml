        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>Update Password</web-resource-name>
            <description/>
            <url-pattern>/UpdatePassword.xhtml</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>My Sessions</web-resource-name>
            <description/>
            <url-pattern>/mySessions.xhtml</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>Mobile Menu</web-resource-name>
            <description/>
            <url-pattern>/mobileMenu.xhtml</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>Index</web-resource-name>
            <description/>
            <url-pattern>/index.xhtml</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>PushMessages</web-resource-name>
            <description/>
            <url-pattern>/javax.faces.push/*</url-pattern>
        </web-resource-collection>
        
        <auth-constraint>
            <description/>
            <role-name>USER</role-name>
            <role-name>DEVELOPER</role-name>
            <role-name>TRAINER</role-name>
            <role-name>ADMIN</role-name>
        </auth-constraint>
    </security-constraint>
    <security-constraint>
        <display-name>Constraint3</display-name>
        <web-resource-collection>
            <web-resource-name>TRAINER Resources</web-resource-name>
            <description/>
            <url-pattern>/trainer/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <description/>
            <role-name>ADMIN</role-name>
            <role-name>TRAINER</role-name>
            <role-name>DEVELOPER</role-name>
        </auth-constraint>
    </security-constraint>
    <security-constraint>
