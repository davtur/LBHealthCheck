                                <h:inputText id="globalFilter" onkeyup="qrtzJobDetailsTable.filter()" />
                            </p:outputPanel>
                        </f:facet>

                        <p:column sortBy="#{item.qrtzJobDetailsPK.schedName}" filterBy="#{item.qrtzJobDetailsPK.schedName}" headerText="#{bundle.ListQrtzJobDetailsTitle_qrtzJobDetailsPK_schedName}"   >
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListQrtzJobDetailsTitle_qrtzJobDetailsPK_schedName}"/>
                            </f:facet>
                            <h:outputText value="#{item.qrtzJobDetailsPK.schedName}"/>
                        </p:column>
