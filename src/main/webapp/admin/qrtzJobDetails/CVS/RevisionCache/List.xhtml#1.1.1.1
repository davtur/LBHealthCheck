                            <h:outputText value=" "/>
                            <p:commandButton rendered="#{qrtzJobDetailsController.isUserInRole('ADMIN')}"  ajax="false" action="#{qrtzJobDetailsController.prepareEdit}" value="#{bundle.ListQrtzJobDetailsEditLink}"/>
                            <h:outputText value=" "/>
                            <p:commandButton rendered="#{qrtzJobDetailsController.isUserInRole('ADMIN')}"  ajax="false" action="#{qrtzJobDetailsController.destroy}" value="#{bundle.ListQrtzJobDetailsDestroyLink}"/>
