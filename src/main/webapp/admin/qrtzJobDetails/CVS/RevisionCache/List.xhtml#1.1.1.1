                <h:panelGroup rendered="#{qrtzJobDetailsController.items.rowCount > 0}">
                    <p:dataTable value="#{qrtzJobDetailsController.items}"  widgetVar="qrtzJobDetailsTable" var="item"  paginator="true" rows="15"  paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"  rowsPerPageTemplate="5,10,15,20,25,50,100" >
                        <f:facet name="header">
